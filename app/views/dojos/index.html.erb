<h1>Dojos#index</h1>
<p>Find me in app/views/dojos/index.html.erb</p>
<p name='notice'><%=notice%></p>

<h2>Listing <%=@dojos.all.count%> Dojos</h2>
<table>
<tr>
    <th>Coding Dojo Branch</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
</tr>
    <% @dojos.each do |dojo| %>
        <tr>
            <td><%=dojo.branch%></td>
            <td><%=dojo.street%></td>
            <td><%=dojo.city%></td>
            <td><%=dojo.state%></td>
        </tr>
    <%end%>
</table>